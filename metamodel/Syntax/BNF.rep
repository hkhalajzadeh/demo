Report 'BNF'
foreach .Non-Terminal; orderby y num
{	id '  :'
	$sep = '  '
	do ~LHS~RHS.() orderby x num
	{	do ~RHS
		{	do ~LHS.() where oid = oid;3
			{	$sep id;2 :Cardinality;1 
				if >Or;1 then $sep = ' | ' endif
			}
		}
	}
	newline
}
foreach .Name; orderby y num, x num unique
{	id '  :  ID' newline
}
endreport